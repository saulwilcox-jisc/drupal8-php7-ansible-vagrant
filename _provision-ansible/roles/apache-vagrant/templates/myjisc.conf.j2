<VirtualHost *:80>
        ServerName myjisc
        ServerAdmin webmaster@localhost
        DocumentRoot "/var/www/myjisc"

        <Directory "/var/www/myjisc">
            AllowOverride All
        </Directory>

        ErrorLog ${APACHE_LOG_DIR}/error.log
        CustomLog ${APACHE_LOG_DIR}/access.log combined
</VirtualHost>
